{
  "name": "ember-cli-misvg",
  "version": "0.3.0",
  "description": "Ember addon to combine SVG files into one JS file and inject it through dedicated component into application. It makes maintenance of SVGs easy and deliver the best possible interaction with SVGs.",
  "keywords": [
    "ember-addon",
    "svg",
    "inline",
    "maintainable"
  ],
  "repository": "git@github.com:vastec/ember-cli-misvg.git",
  "author": "Tomasz Nie≈ºurawski <tomasz@niezurawski.pl>",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "^1.1.0",
    "broccoli-merge-trees": "^1.2.1",
    "broccoli-misvg": "0.1.1",
    "broccoli-unwatched-tree": "^0.1.1",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "make-array": "^1.0.1",
    "merge": "^1.2.0"
  },
  "devDependencies": {
    "ember-ajax": "^3.0.0",
    "ember-cli": "^3.1.4",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "ember-welcome-page": "^3.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
